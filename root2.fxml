	<children>
		<imageView fitWidth = "200" preserveRatio = "true">
			<image <image url ="images/javafx.jpg" /> </image> 
		</imageView>
	<HBox alignment = "CENTER" spacing = "20.0">
		<children>
			<Button text = "이전" />
